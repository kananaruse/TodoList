<h1><%= @list.title %></h1>
<%= form_for ( [@list, @list.tasks.build] ), url: list_task_path, method: :patch do |f| %>
  <p>
    <%= f.label "ToDoを編集" %> <br>
    <%= f.label :name, "ToDo名" %>
    <%= f.text_field :name, value: @task.name %>
  </p>

  <p>
    <%= f.label :due_date, "期限日" %>
    <%= f.text_field :due_date%>
  </p>

  <p>
    <%= f.submit %>
  </p>
  <%= render 'layouts/error_messages', model: f.object %>
<% end %>
